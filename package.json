{
  "name": "blockchain-fullstack-app",
  "version": "1.0.0",
  "description": "blockchain fullstack app",
  "main": "index.js",
  "scripts": {
    "compile": "tsc && node dist/index.js",
    "server": "npm run start:redis && cross-env ENV='development' nodemon -e ts --exec \"npm run compile\"",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev:peer": "cross-env GENERATE_PEER_PORT='true' ENV='development' nodemon -e ts --exec \"npm run compile\"",
    "start:redis": "redis-server --daemonize yes",
    "build": "tsc",
    "test:watch": "jest --watchAll",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elPoeta/blockchain-fullstack-app.git"
  },
  "keywords": [
    "blockchain",
    "node",
    "react",
    "express",
    "redis"
  ],
  "author": "Leonardo Tosetto (leonardo.a.tosetto@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elPoeta/blockchain-fullstack-app/issues"
  },
  "homepage": "https://github.com/elPoeta/blockchain-fullstack-app#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.15",
    "@types/node": "^17.0.38",
    "cross-env": "^7.0.3",
    "jest": "^26.6.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "express": "^4.18.1",
    "redis": "^4.1.0"
  }
}